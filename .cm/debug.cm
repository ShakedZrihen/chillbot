# -*- mode: yaml -*-

manifest:
  version: 1.0

triggers:
  on:
    - label_added
  exclude:
    branch:
      - "patch"

automations:
  checklist: 
    if:
      - true
    run:
      - action: add-comment@v1
        args:
          comment: "this branch was not excluded, labels: {{ pr.labels | dump | safe }}"
